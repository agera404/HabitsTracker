<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.Material3.Light.NoActionBar">

    </style>

    <style name="ToolBarStyle" parent="AppTheme">
        <item name="android:background">@color/colorPrimary</item>
        <item name="android:textSize" />
        <item name="titleTextColor">@color/textColorSecondary</item>
        <item name="title">@string/app_name</item>
    </style>
    <style name="buttonStyle" parent="AppTheme">
        <item name="android:textColor">@color/textColorSecondary</item>
        <item name="android:background">@drawable/button_round_border</item>
        <item name="android:layout_margin">2dp</item>
    </style>
    <style name="buttonStyle2" parent="AppTheme">
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:background">@drawable/button_round_border_without_background</item>
        <item name="android:layout_margin">2dp</item>
    </style>
    <style name="editText" parent="AppTheme">
        <item name="android:background">@drawable/round_border</item>
        <item name="android:padding">10dp</item>
        <item name="android:layout_margin">2dp</item>
    </style>

</resources>